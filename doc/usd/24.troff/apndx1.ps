$troff begin
/saveMatrix matrix def
/idMatrix matrix def
/bd {	% <num> bd
	dup /bdx exch def
	0 dtransform ceiling /bdY exch def ceiling /bdX exch def
	saveMatrix currentmatrix pop
} bind def
/Pbd {	% like P, but with bd applied
	3 1 roll moveto idMatrix setmatrix currentpoint round exch round exch
	2 copy moveto saveMatrix setmatrix bdx 0 4 index ashow
	idMatrix setmatrix moveto bdX bdY rmoveto
	saveMatrix setmatrix bdx 0 3 -1 roll ashow
} bind def
end
